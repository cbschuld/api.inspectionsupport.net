{
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "required": true,
            "description": "id of the contact"
        },
        "first": {
            "type": "string",
            "required": true,
            "description": "first name of the contact"
        },
        "last": {
            "type": "string",
            "required": true,
            "description": "last name of the contact"
        },
        "display": {
            "type": "string",
            "required": true,
            "description": "display name of the contact"
        },
        "email": {
            "type": "string",
            "required": true,
            "description": "email address of the contact"
        },
        "url": {
            "type": "string",
            "required": true,
            "description": "url for the contact"
        },
        "address1": {
            "type": "string",
            "required": true,
            "description": "address (first line) of the contact"
        },
        "address2": {
            "type": "string",
            "required": true,
            "description": "address (second line) of the contact"
        },
        "city": {
            "type": "string",
            "required": true,
            "description": "the contact's city"
        },
        "state": {
            "type": "string",
            "required": true,
            "description": "the contact's state"
        },
        "stateabbreviation": {
            "type": "string",
            "required": true,
            "description": "the contact's state (abbreviation)"
        },
        "zip": {
            "type": "string",
            "required": true,
            "description": "zip/postal code for the contact"
        },
        "workphone": {
            "type": "string",
            "required": true,
            "description": "work phone number for the contact"
        },
        "homephone": {
            "type": "string",
            "required": true,
            "description": "home phone number for the contact"
        },
        "mobilephone": {
            "type": "string",
            "required": true,
            "description": "cell / mobile phone for the contact"
        },
        "workfax": {
            "type": "string",
            "required": true,
            "description": "work fax number for the contact"
        },
        "homefax": {
            "type": "string",
            "required": true,
            "description": "the contact's home fax number"
        },
        "notes": {
            "type": "string",
            "required": true,
            "description": "notes for the contact"
        },
        "show": {
            "type": "boolean",
            "required": true,
            "description": "false if the contact is deleted otherwise true"
        },
        "modified": {
            "type": "string",
            "format": "date-time",
            "required": true,
            "description": "the last date/time this contact was modified (in UTC)"
        }
    }
