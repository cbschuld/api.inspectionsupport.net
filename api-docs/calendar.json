{
    "apiVersion": "1.0",
    "swagger": "2.0",
    "basePath": "https://inspectionsupport.net/test/rest",
    "resourcePath": "/calendarevents",
    "apis": [
        {
            "path": "/availableslots",
            "description": "Obtain available calendar slots for appointments",
            "operations": [
                {
                    "httpMethod": "GET",
                    "summary": "Obtains the inspector's available appointment slots",
                    "responseClass": "Collection[ResponseAvailabilitySlot]",
                    "nickname": "availableslots",
                    "parameters": [
                        {
                            "name": "username",
                            "description": "A username -or- order access key - for an example use 'bsmith'",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        },
                        {
                            "name": "password",
                            "description": "A password -or- order secret access key - for an example use 'password'",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        },
                        {
                            "name": "daysahead",
                            "description": "The number of upcoming days to show availability for.",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "integer"
                        },
                        {
                            "name": "inspector",
                            "description": "The ID of the inspector",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        }
                    ]
                }
            ]
        }
    ],
    "models": {
        "Collection[ResponseAvailabilitySlot]": {
            "properties": {
                "empty": {
                    "type": "boolean"
                }
            },
            "type": "Collection[ResponseAvailabilitySlot]"
        },
        "ResponseAvailabilitySlot": {
            "properties": {
                "userid": {
                    "type": "string",
                    "required": true,
                    "description": "id of user / inspector"
                },
                "start": {
                    "type": "datetime",
                    "required": true,
                    "description": "the appointment start date/time (format is M/D/YYYY HH:MM)"
                },
                "end": {
                    "type": "datetime",
                    "required": true,
                    "description": "the appointment end date/time (format is M/D/YYYY HH:MM)"
                }
            },
            "id": "ResponseAvailabilitySlot"
        }
    }
}
