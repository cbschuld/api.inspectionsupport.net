{
    "apiVersion": "1.0",
    "swagger": "2.0",
    "basePath": "https://inspectionsupport.net/test/rest",
    "resourcePath": "/agent",
    "apis": [
        {
            "path": "/agent/{id}",
            "description": "Obtain the agent",
            "operations": [
                {
                    "httpMethod": "GET",
                    "summary": "Obtains the agent for the given id",
                    "responseClass": "ResponseAgent",
                    "nickname": "agent_get",
                    "parameters": [
                        {
                            "name": "id",
                            "description": "The ID of the agent",
                            "paramType": "path",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        },
                        {
                            "name": "username",
                            "description": "A username -or- order access key - for an example use 'bsmith'",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        },
                        {
                            "name": "password",
                            "description": "A password -or- order secret access key - for an example use 'password'",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/agent/{id}",
            "description": "Delete this agent",
            "operations": [
                {
                    "httpMethod": "DELETE",
                    "summary": "Deletes the agent",
                    "responseClass": "void",
                    "nickname": "agent_delete",
                    "parameters": [
                        {
                            "name": "id",
                            "description": "The ID of the agent",
                            "paramType": "path",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        },
                        {
                            "name": "username",
                            "description": "A username -or- order access key - for an example use 'bsmith'",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        },
                        {
                            "name": "password",
                            "description": "A password -or- order secret access key - for an example use 'password'",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        }
                    ]
                }
            ]
        }
    ],
    "models": {

        "ResponseAgent": {
            "properties": {
                "id": {
                    "type": "string",
                    "required": true,
                    "description": "id of agent"
                },
                "agency": {
                    "type": "string",
                    "required": true,
                    "description": "office of agent (agency)"
                },
                "firstname": {
                    "type": "string",
                    "required": true,
                    "description": "first name of agent"
                },
                "lastname": {
                    "type": "string",
                    "required": true,
                    "description": "last name of agent"
                },
                "displayname": {
                    "type": "string",
                    "required": true,
                    "description": "display name of agent"
                },
                "email": {
                    "type": "string",
                    "required": true,
                    "description": "email address of agent"
                },
                "url": {
                    "type": "string",
                    "required": true,
                    "description": "url for the agent"
                },
                "address1": {
                    "type": "string",
                    "required": true,
                    "description": "address (first line) of the agent"
                },
                "address2": {
                    "type": "string",
                    "required": true,
                    "description": "address (second line) of the agent"
                },
                "city": {
                    "type": "string",
                    "required": true,
                    "description": "agent's city"
                },
                "state": {
                    "type": "string",
                    "required": true,
                    "description": "agent's state"
                },
                "stateabbreviation": {
                    "type": "string",
                    "required": true,
                    "description": "agent's state (abbreviation)"
                },
                "zip": {
                    "type": "string",
                    "required": true,
                    "description": "zip/postal code for the agent"
                },
                "workphone": {
                    "type": "string",
                    "required": true,
                    "description": "work phone number for the agent"
                },
                "homephone": {
                    "type": "string",
                    "required": true,
                    "description": "home phone number for the agent"
                },
                "mobilephone": {
                    "type": "string",
                    "required": true,
                    "description": "cell / mobile phone for the agent"
                },
                "workfax": {
                    "type": "string",
                    "required": true,
                    "description": "work fax number for the agent"
                },
                "homefax": {
                    "type": "string",
                    "required": true,
                    "description": "agent's license number"
                },
                "notes": {
                    "type": "string",
                    "required": true,
                    "description": "notes for the agent"
                },
                "show": {
                    "type": "boolean",
                    "required": true,
                    "description": "false if the agent is deleted"
                },
                "modified": {
                    "type": "datetime",
                    "required": true,
                    "description": "the last date/time this agent was modified (in UTC)"
                }
            },
            "id": "ResponseAgent"
        }
    }
}
